<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="item.entity.Orderitem" table="orderitem" schema="farm_produce_shop">
        <id name="itemid" column="itemid"/>
        <property name="count" column="count"/>
        <property name="subtotal" column="subtotal"/>
        <many-to-one name="product" column="pid" class="product.entity.Product"/>
        <many-to-one name="order" column="oid" class="order.entity.Orders"/>
    </class>
</hibernate-mapping>